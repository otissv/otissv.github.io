    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.3.5",
    "browser-sync": "^2.12.8",
    "browserify": "^13.0.1",
    "handlebars": "^4.0.5",
    "node-sass": "^4.14.1",
    "ramda": "^0.21.0",
    "watchify": "^3.7.0"
